PROGRAM Cylinder
	/*Missing END of IF statement*/
	INT r, a, p, h; 
	r = 8;
	h = 10;
	a = 0;
	FLOAT surface, volume;
	IF ( r > 5 && h > 10) THEN
	  volume = 3.14 * r * r * h;
	  surface = 2 * 3.14 * r * h + 2 * 3.14 * r * r;
	 
	/*Display the results*/
	PRINT  ("The output results are " , volume, " ", surface, " " , h, " ", r);
END  PROGRAM
