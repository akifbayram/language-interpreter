PROGRAM Cylinder
	/*Illegal Equality Expression*/
	INT r, a, h, b; 
	r = 8;
	h = 10;
	a = 0;
	FLOAT surface, volume;
	BOOL flag;
	IF ( r > 5 && h > 10) THEN
	  volume = 3.14 * r * r * h;
	  surface = 2 * 3.14 * r * h + 2 * 3.14 * r * r;
	END IF; 
	flag = (r == h == 10);
	/*Display the results*/
	PRINT  ("The output results are " , volume, " ", surface, " flag: " , flag);
END  PROGRAM
