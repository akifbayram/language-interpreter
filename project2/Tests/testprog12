PROGRAM Cylinder
	/*Missing operand after operator*/
	INT r, a, h, b; 
	r = 8;
	h = 10;
	a = 0;
	FLOAT surface, volume;
	IF ( r > 5 && h > 10) THEN
	  volume = 3.14 * r * r * h;
	  surface = 2 * 3.14 * r * h +  * 3.14 * r * r;
	END IF; 
	/*Display the results*/
	PRINT  ("The output results are " , volume, " ", surface, " " , h, " ", r);
END  PROGRAM
